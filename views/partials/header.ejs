        <%# Alert Section %>  
		<div class="container">	
			<%# Error %>  
			<% if( error && error.length > 0 ) { %>
			<div class="alert alert-danger" role="alert">
				<%= error %>
			</div>
			<% } %>
			<%# Success %>  
			<% if( success && success.length > 0 ) { %>
			<div class="alert alert-success" role="success">
				<%= success %>
			</div>
			<% } %>			
		</div>
